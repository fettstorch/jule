{
  "name": "@fettstorch/jule",
  "version": "1.5.3",
  "description": "Some general JS/TS utils",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schnullerpip/jule.git"
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun run build:esm && bun run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "vitest",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply .",
    "update-deps": "bunx npm-upgrade",
    "prepack": "bun run build && bun run test -- --run"
  },
  "keywords": ["typescript", "utils", "when"],
  "author": "Julian MÃ¼ller",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "bun": "^1.1.34",
    "vitest": "^2.1.4"
  }
}
